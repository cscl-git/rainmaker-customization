{
  "key": "store-asset-report-opening-balance",
  "DataConfigs": {
    "serviceName": "rainmaker-common",
    "version": "1.0.0",
    "baseKeyPath": "$.OpeningBalanceReport.*",
    "entityIdPath": "$.storeName",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
			{
                "variable": "ulbLogo",                        
                "url":"https://egov.chandigarhsmartcity.in/employee/static/media/pblogo.2feffb62.png",
                "type":"image"
                },
              {
                "variable": "storeName",
                "value": {
                  "path": "$.storeName"
                }
              },
			  {
                "variable": "financialYear",
                "value": {
                  "path": "$.financialYear"
                }
              },
              {
                "variable": "variable1",
                "value":{
                     "path":"$.balanceDetails.*"
                 },
                 "type":"array",
                 "format":{
                  "type":"string",
                  "scema":[
                    {
					  "variable": "srNo",
					  "value": "srNo"
					},
					{
					  "variable": "materialCode",
					  "value": "materialCode"
					},
					{
					  "variable": "materialName",
					  "value": "materialName"
					},{
					  "variable": "materialType",
					  "value": "materialType"
					},{
					  "variable": "uomName",
					  "value": "uomName"
					},{
					  "variable": "quantity",
					  "value": "quantity"
					},{
					  "variable": "unitRate",
					  "value": "unitRate"
					},{
					  "variable": "totalAmount",
					  "value": "totalAmount"
					},{
					  "variable": "remarks",
					  "value": "remarks"
					}
                  ]
                }
              },
			  {
                "variable": "variable2",
                "value":{
                     "path":"$.workflowDetails.*"
                 },
                 "type":"array",
                 "format":{
                  "type":"string",
                  "scema":[
                    {
					  "variable": "ReviewApprovalDate",
					  "value": "reviewApprovalDate"
					},
					{
					  "variable": "ReviewerApproverName",
					  "value": "reviewerApproverName"
					},{
					  "variable": "Designation",
					  "value": "designation"
					},{
					  "variable": "Action",
					  "value": "action"
					},{
					  "variable": "SendTo",
					  "value": "sendTo"
					},{
					  "variable": "ApprovalStatus",
					  "value": "approvalStatus"
					}
                  ]
                }
              }
            ]
          }
        ]
      }
    ]
  }
}