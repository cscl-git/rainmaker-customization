{

   "key":"advertisement-receipt",
   "DataConfigs": {
      "serviceName": "pm-services",
      "version": "1.0.0",
      "baseKeyPath": "$.nocApplicationDetail.*",
      "entityIdPath":"$.applicationId",
      "isCommonTableBorderRequired":true,
      "mappings": [
         {
            "mappings": [
               {
                  "direct": [
				  {
                        "variable": "variable33",                        
                        "url":"http://mcchandigarh.gov.in/sites/default/files/mc-chandigardh-logo.png",
                        "type":"image"
                     },
                     {
                        "variable": "variable1",
                        "value": {
                             "path":"$.totalAmount"                              
                        }
                     },            
                     {
                        "variable":"variable2",
                        "value":{
                           "path":"$.GSTIN"                            
                        }  
                     },
                     {
                        "variable": "variable3",
                        "value":{
