{

	"key": "challanReceipt-ec",
	"DataConfigs": {
		"serviceName": "egov-ec",
		"version": "1.0.0",
		"baseKeyPath": "$.paymentEchallan.*",
		"entityIdPath": "$.applicationId",
		"isCommonTableBorderRequired": true,
		"mappings": [{			
			"mappings": [{
				"direct": [
					{
					"variable": "ulbLogo",                        
					"url":"https://egov.chandigarhsmartcity.in/employee/static/media/pblogo.2feffb62.png",
					"type":"image"
					},
					{
						"variable": "variable1",
						"value": {
							"path": "$.receiptNo"
						}
					},
					{
						"variable": "variable2",
						"value": {
							"path": "$.dated"
						}
					},
					{
						"variable": "variable3",
						"value": {
							"path": "$.violatorName"
						}
					},
					{
						"variable": "variable4",
						"value": {
							"path": "$.amount"
						}
					},
					{
						"variable": "variable5",
						"value": {
							"path": "$.amountInWord"
						}
					},
					{
						"variable": "variable6",
						"value": {
							"path": "$.paymentMode"
						}
					},
					{
						"variable": "variable7",
						"value": {
							"path": "$.memoNo"
						}
					},
					{
                "variable": "variable8",
                "value": {
                  "path": "$.itemDetails.*"
                },
                "type": "array",
                "format": {
                  "type": "string",
                  "scema": [
                    {
                      "variable": "itemName",
                      "value": "itemName"
                    },
					{
                      "variable": "quantity",
                      "value": "quantity"
                    }
                  ]
                }
              }
				]
			}]
		}]
	}
}